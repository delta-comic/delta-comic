{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "clipboard-manager:allow-read-text",
    "clipboard-manager:deny-write-text",
    "fs:default",
    "fs:allow-applocaldata-write-recursive",
    "sql:default",
    "sql:allow-execute",
    "upload:default",
    "pinia:default",
    "core:event:default",
    "m3:default",
    "cors-fetch:default"
  ]
}